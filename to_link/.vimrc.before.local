" which bundle groups to install
let g:spf13_bundle_groups=['general', 'programming', 'javascript', 'html', 'neocomplcache', 'twig']

" either of these break phpcomplete_extended
" prevent autocd into file directory
let g:spf13_no_autochdir = 1
let g:spf13_no_views = 1
let g:spf13_clear_search_highlight = 1

" keep H and L shortcuts
let g:spf13_no_fastTabs = 1

" vim-tags
" set exrc
" set secure
