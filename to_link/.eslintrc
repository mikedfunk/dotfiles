{
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "generators": true,
    "globalReturn": true,
    "jsx": true,
    "modules": true,
    "spread": true,
    "superInFunctions": true
  },
  "rules": {
    "accessor-pairs": 1,
    "strict": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "guard-for-in": 0,
    "vars-on-top": 0,
  }
}
