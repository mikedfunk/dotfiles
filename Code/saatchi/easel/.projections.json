{
  "**/containers/*.js": {
    "alternate": [
      "{dirname}/specs/{basename}.spec.js",
      "{}.spec.js",
      "{dirname}/../components/{basename}.js"
    ],
    "type": "container"
  },
  "**/components/*.js": {
    "alternate": [
      "{dirname}/specs/{basename}.spec.js",
      "{}.spec.js",
      "{dirname}/../containers/{basename}.js"
    ],
    "type": "component"
  },
  "*.spec.js": {
    "alternate": [
      "{dirname}/../{basename}.js",
      "{}.js"
    ],
    "type": "spec"
  },
  "*.js": {
    "alternate": [
      "{dirname}/specs/{basename}.spec.js",
      "{}.spec.js"
    ]
  }
}
