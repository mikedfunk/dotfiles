# }}}

# mysql general query log {{{
colorscheme:mysqllog
# date, time
cs_re:magenta:^[0-9]+\-[0-9]+\-[0-9]+
mcsre_s:yellow:^[0-9\-]+T([0-9:]+)\.[0-9]+Z.*$
# ignore this stuff
mcsre_s:black:^[0-9\-]+(T)
mcsre_s:black:^[-0-9:T]+(\.[A-Z0-9]+)
cs_re:black:Query
# query time
mcsre_s:magenta,,bold:([0-9]+) Query
# queries
cs_re:white:(START TRANSACTION|commit)
cs_re:cyan:(SELECT).*
cs_re:green:(INSERT).*
cs_re:yellow:(UPDATE).*
cs_re:red:(DELETE).*
# }}}
